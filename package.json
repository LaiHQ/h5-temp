{
  "name": "h5-temp",
  "version": "1.0.0",
  "description": "H5移动端",
  "scripts": {
    "dev": "vite",
    "build:dev": "vite build --mode development",
    "build:uat": "vite build --mode uat",
    "build:staging": "vite build --mode staging",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs --config /docs/vite.config.js",
    "docs:build": "vitepress build docs --config /docs/vite.config.js",
    "docs:serve": "vitepress serve docs --config /docs/vite.config.js",
    "lint": "eslint --fix --ext .js,.ts,.tsx,.vue src",
    "format": "prettier --write \"src/**/*.js\" \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.vue\"",
    "prepare": "husky install",
    "commit": "git cz",
    "release": "standard-version && git push --follow-tags",
    "lint:style": "stylelint --fix \"src/**/*.{css,less,vue}\"",
    "preinstall": "npx only-allow pnpm"
  },
  "browserslist": "ie >= 11",
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "animate.css": "^4.1.1",
    "ant-design-vue": "^3.2.15",
    "axios": "^1.3.4",
    "fastclick": "^1.0.6",
    "less": "^4.1.3",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.32",
    "pinia-plugin-persistedstate": "^3.1.0",
    "qrcode": "^1.5.3",
    "qs": "^6.11.1",
    "secure-ls": "^1.2.6",
    "typescript": "^4.9.5",
    "vant": "^4.5.0",
    "vconsole": "^3.15.1",
    "vite": "^4.1.3",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "web-vitals": "^3.1.1",
    "wow.js": "^1.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/fastclick": "^1.0.30",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-legacy": "^4.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "amfe-flexible": "^2.2.1",
    "autoprefixer": "^10.4.14",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.34.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.24",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.8.4",
    "qrcode-terminal": "^0.12.0",
    "rollup-plugin-commonjs": "^10.1.0",
